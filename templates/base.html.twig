<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>
			{% block title %}Welcome!
			{% endblock %}
		</title>
		<link rel="icon" href="data:image/svg+xml,<svg xmlns=%22http://www.w3.org/2000/svg%22 viewBox=%220 0 128 128%22><text y=%221.2em%22 font-size=%2296%22>⚫️</text></svg>">
		<link rel="preconnect" href="https://fonts.googleapis.com">
		<link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
		<link
		href="https://fonts.googleapis.com/css2?family=Raleway:wght@200&display=swap" rel="stylesheet">
		{# Run `composer require symfony/webpack-encore-bundle` to start using Symfony UX #}
		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
			<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/bootswatch/5.1.3/sandstone/bootstrap.min.css"/>
			<link href="{{ asset('styles/style.css') }}" type="text/css" rel="stylesheet">
			<link href="{{ asset('styles/header.css') }}" type="text/css" rel="stylesheet">
			<link href="{{ asset('styles/footer.css') }}" type="text/css" rel="stylesheet">
			<link href="{{ asset('styles/index.css') }}" type="text/css" rel="stylesheet">
			<link href="{{ asset('styles/carousel.css') }}" type="text/css" rel="stylesheet">
			<link href="{{ asset('styles/offres.css') }}" type="text/css" rel="stylesheet">
			<link href="{{ asset('styles/register.css') }}" type="text/css" rel="stylesheet">
			<link href="{{ asset('styles/project.css') }}" type="text/css" rel="stylesheet">
		{% endblock %}

	</head>
	<body>
		{% block header %}
			{% include "partials/_header.html.twig" %}
		{% endblock %}
		{% block body %}{% endblock %}
		{% block footer %}
			{% include "partials/_footer.html.twig" %}
		{% endblock %}
		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
			<script src="https://code.jquery.com/jquery-3.6.1.min.js" integrity="sha256-o88AwQnZB+VDvE9tvIXrMQaPlFFSUTR+nldQm1LuPXQ=" crossorigin="anonymous"></script>
			<script src="https://kit.fontawesome.com/8769df2668.js" crossorigin="anonymous"></script>
			<script src="{{ asset('assets/js/app.js') }}"></script>
			<script type="text/javascript" src="{{ asset('assets/js/bootstrap.min.js') }}"></script>

		{% endblock %}
	</body>
</html>
